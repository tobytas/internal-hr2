<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TE06_Existing_Type_can_not_be_changed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 NOT(ISNEW()),
 ISCHANGED(Type__c)
)</errorConditionFormula>
    <errorDisplayField>Type__c</errorDisplayField>
    <errorMessage>TE06: Type can not be changed for existing Time Entry</errorMessage>
</ValidationRule>
